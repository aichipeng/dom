{
    "presets": [
        "env",
        "react",
        "es2015",
        "stage-0"
    ],
    "env": {
        "development": {
            "presets": [
                "react-hmre"
            ]
        }
    },
    "plugins": [
        [
            "import",
            {
                "libraryName": "antd",
                "libraryDirectory": "es",
                "style": "css" // `style: true` 会加载 less 文件
            }
        ]
    ]
}